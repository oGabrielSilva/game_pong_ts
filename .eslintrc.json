{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base", "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-useless-constructor": "off",
    "no-empty-function": "off",
    "class-methods-use-this": "off",
    "no-nested-ternary": "off"
  }
}
